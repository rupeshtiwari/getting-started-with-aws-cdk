{
    "spellchecker.ignoreWordsList": [
        "GitHub",
        "VSCode",
        "cloudformation",
        "mkdir",
        "CloudFormation"
    ],
    "explorer.experimental.fileNesting.enabled": true,
    "files.exclude": {
        // Hide all files that end with .js if they have a .ts file
        "**/*.js": {
            "when": "$(basename).ts"
        },
        // Hide all files that end in .d.ts
        "**/*.d.ts": true,
    }
}